{
  "author": "Declarative DOM Working Group",
  "bugs": {
    "url": "https://github.com/declarative-dom/ddom/issues"
  },
  "description": "Library for building UI components using a Declarative DOM JavaScript object literals.",
  "devDependencies": {
    "@rsdoctor/rspack-plugin": "^1.3.11",
    "@rslib/core": "^0.18.0",
    "signal-polyfill": "^0.2.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "typia": "^10.0.2",
    "typia-rspack-plugin": "^2.2.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "homepage": "https://github.com/declarative-dom/ddom/tree/main/lib#readme",
  "keywords": [
    "declarative",
    "dom",
    "components",
    "spec",
    "ui",
    "typescript",
    "json",
    "data-structure"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@declarative-dom/lib",
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/declarative-dom/ddom.git"
  },
  "scripts": {
    "build": "rslib build",
    "build:analyze": "RSDOCTOR=true rslib build",
    "clean": "rimraf dist",
    "debug": "cross-env NO_COLOR=true rslib build",
    "dev": "rslib build --watch",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "test": "cd .. && npm run test",
    "upgrade": "npx npm-check-updates -u && npm install"
  },
  "type": "module",
  "types": "dist/src/index.d.ts",
  "version": "0.7.2"
}
